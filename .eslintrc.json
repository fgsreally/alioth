{
  "extends": "@antfu/eslint-config",
  
  "parserOptions": {
    "emitDecoratorMetadata": true
  },
  "rules":{
    "vue/no-dupe-keys":"off",
    "vue/valid-v-model":"off",
    "vue/prop-name-casing":"off",
    "vue/no-extra-parens":"off",
      "vue/no-mutating-props": ["error", {
      "shallowOnly": true
    }]
  }
}